</head>

<body>

<div id="site-wrapper">

  <div id="header">

    <div id="top">

      <div class="left" id="logo">
        <%= link_to image_tag("logo.gif"), home_index_path %>
      </div>

      <div class="left navigation" id="main-nav">

        <ul class="tabbed">
          <li class="current-tab"><a href="index.html">News</a></li>
          <li><a href="#">Notifications</a></li>
        </ul>

        <div class="clearer">&nbsp;</div>

      </div>

      <div class="clearer">&nbsp;</div>

    </div>

    <div class="navigation" id="sub-nav">

      <ul class="tabbed">
        <li><%=link_to 'Change Issuing', :controller=>"changes", :action=>"index"%></li>
        <li><a href="style-demo.html">Review/Approval</a></li>
        <li><%=link_to 'Admin', :controller=>"home", :action=>"admin_index"%></li>
      </ul>

      <div class="clearer">&nbsp;</div>

    </div>

  </div>



 <%=yield%>

  <div id="footer">

    <div class="left" id="footer-left">
      
      <%= image_tag("logo-small.gif") %>
      
